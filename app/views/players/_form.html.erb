<%= form_for @player do |f| %>
  <p class="name">
    <%= f.label :username %>
    <%= f.text_field :username %>
  </p>
  <p class="name">
    <%= f.label :full_name, "Nombre Completo" %>
    <%= f.text_field :full_name %>
  </p>
  <p class="email">
    <%= f.label :email %>
    <%= f.text_field :email %>
  </p>
  <p class="text">
    <%= f.label :birth_at, "Fecha de Nacimiento" %>
    <%= f.datetime_select :birth_at %>
  </p>
  <p class="submit">
    <%= f.submit %>
  </p>
<% end %>
<p>
  <%= link_to "Back", players_path %>
</p>
